{# DO NOT EDIT THIS FILE #}	
{% extends 'base/layout/pagelayout.html.twig' %}

{% block title %}G6K Demo - {{ step.simulator.label}}{% endblock %}

{% block metas %}
{{ parent() }}<meta name="viewport" content="width=device-width, initial-scale=1.0" />
{% include 'Demo/sections/meta.html.twig' %}
{% endblock %}

{% block styles %}
{{ parent() }}
<link rel='stylesheet' type='text/css' href="{{ asset('assets/base/js/libs/bootstrap/css/bootstrap.css') }}" />
{% include 'Demo/sections/favicon.html.twig' %}
{% include 'Demo/sections/styles.html.twig' %}
{% endblock %}

{% block body %}
	{% include 'Demo/sections/header.html.twig' %}
	<main class="main" id="main" role="main">
		<div class="container">
			{% block content %}{% endblock %}
		</div>
	</main>
	{% include 'Demo/sections/footer.html.twig' %}
	{% block scripts %}
		{{ parent() }}
		<script type="text/javascript">
		$(function(){
			setTimeout(function() {
				$('input.date[data-widget=abDatepicker], input.date[data-widget=abCoupledDatepicker]').datepicker('theme', 'blue');
				$('input.listbox-input').listbox('theme', 'blue');	
			}, 0);
		});
		</script>
		{% include 'Demo/sections/scripts.html.twig' %}
	{% endblock %}
{% endblock %}
